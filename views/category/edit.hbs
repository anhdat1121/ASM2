<form action="/category/edit/{{ category._id }}" method="post">
   <h1>Edit category</h1>
   Category name: <input type="text" name="name" id="" value="{{ category.name }}" required>
   <br><br>
   Category material: <input type="text" name="material" id="" value="{{ category.material }}" required>
   <br><br>
   <input type="submit" class="btn btn-success"value="Edit">
</form>